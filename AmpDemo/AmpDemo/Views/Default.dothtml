@viewModel AmpDemo.ViewModels.DefaultViewModel, AmpDemo
@masterPage Views/MasterPage.dotmaster
<dot:Content ContentPlaceHolderID="MainContent">
    <img src="/DotVVM.AMP.png" />
    <dot:HtmlLiteral Html="{value: Html}"/>
    <dot:GridView DataSource="{value: Data}">
        <dot:GridViewTextColumn HeaderText="Index" ValueBinding="{value: Index}" />
        <dot:GridViewTextColumn HeaderText="Text" ValueBinding="{value: Text}" />
    </dot:GridView>
    
</dot:Content>
